    {
        "name": "TYPEFORM__UPDATE_FORM_STRUCTURE",
        "description": "Updates the structure of an existing form, including adding or modifying its questions.",
        "tags": ["typeform", "forms", "update", "questions", "structure"],
        "visibility": "public",
        "active": true,
        "protocol": "rest",
        "protocol_data": {
            "method": "PUT",
            "path": "/forms/{form_id}",
            "server_url": "https://api.typeform.com"
        },
        "parameters": {
            "type": "object",
            "properties": {
                "path": {
                    "type": "object",
                    "description": "URL path parameters",
                    "properties": {
                        "form_id": {
                            "type": "string",
                            "description": "The unique ID of the form to update."
                        }
                    },
                    "required": ["form_id"],
                    "visible": ["form_id"],
                    "additionalProperties": false
                },
                "body": {
                    "type": "object",
                    "description": "The JSON definition of the form's structure.",
                    "properties": {
                        "title": {
                            "type": "string",
                            "description": "The title of the form."
                        },
                        "fields": {
                            "type": "array",
                            "description": "The list of questions (fields) for the form.",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "title": {
                                        "type": "string",
                                        "description": "The question text."
                                    },
                                    "type": {
                                        "type": "string",
                                        "description": "The type of question.",
                                        "enum": ["short_text", "long_text", "multiple_choice", "rating", "yes_no"]
                                    },
                                    "properties": {
                                        "type": "object",
                                        "description": "Question-specific properties.",
                                        "properties": {
                                            "choices": {
                                                "type": "array",
                                                "description": "A list of choices for multiple_choice questions.",
                                                "items": {
                                                    "type": "object",
                                                    "properties": {
                                                        "label": {
                                                            "type": "string"
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                },
                                "required": ["title", "type"]
                            }
                        }
                    },
                    "required": ["title", "fields"],
                    "visible": ["title", "fields"],
                    "additionalProperties": false
                }
            },
            "required": ["path", "body"],
            "visible": ["path", "body"],
            "additionalProperties": false
        }
    },
